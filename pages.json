{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index",
      "style": {
        "navigationBarTitleText": "土豆计时器",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/setting",
      "style": {
        "navigationBarTitleText": "设置",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/singleTimerList",
      "style": {
        "navigationBarTitleText": "已保存单次计时器",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/editTimerItem",
      "style": {
        "navigationBarTitleText": "编辑计时器",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/loopTimerGroupList",
      "style": {
        "navigationBarTitleText": "已保存循环计时器",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/singleCountDown",
      "style": {
        "navigationBarTitleText": "单次计时器",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/loopCountDown",
      "style": {
        "navigationBarTitleText": "循环计时器",
        "navigationStyle": "custom"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "土豆计时器",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "disableScroll": true
  },
  "easycom": {
    // 下载安装的方式需要前面的"@/"，npm安装的方式无需"@/"
    // 下载安装方式
    // "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
    // npm安装方式
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  }
}
